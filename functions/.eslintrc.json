{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "google"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "root": true,
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": "off",
    "indent":"off",
    "linebreak-style": "off",
    "max-len": ["warn", 140],
    "object-curly-spacing": ["error", "always"],
    "padded-blocks": "off",
    "require-jsdoc": "off"
  }
}
